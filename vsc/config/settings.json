{
    "editor.fontFamily": "Operator Mono for Powerline",
    "editor.fontSize": 12,
    "editor.tabSize": 2,
    "editor.wordWrap": true,
    "editor.quickSuggestionsDelay": 5,
    "editor.tabCompletion": true,
    "window.reopenFolders": "none",
    "files.associations": {
      "*.zsh-theme": "shellscript"
    },
    "window.zoomLevel": 0,
    "search.exclude": {
      "**/node_modules": true,
      "**/bower_components": true,
      "**/components":true
    }
}