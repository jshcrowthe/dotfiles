// Place your key bindings in this file to overwrite the defaults
[
    { "key": "ctrl+cmd+up", "command": "editor.action.moveLinesUpAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "ctrl+cmd+down", "command": "editor.action.moveLinesDownAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "alt+up", "command": "-editor.action.moveLinesUpAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "alt+down", "command": "-editor.action.moveLinesDownAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "ctrl+enter", "command": "code-runner.run", "when": "editorTextFocus && resourceLangId == javascript"},
    { "key": "cmd+shift+g", "command": "workbench.view.git" },
    { "key": "ctrl+shift+g", "command": "-workbench.view.git" },
    { "key": "shift+cmd+\\", "command": "-editor.action.jumpToBracket", "when": "editorTextFocus" }
    // { "key": "ctrl+m", "command": "editor.action.jumpToBracket", "when": "editorTextFocus" }
]